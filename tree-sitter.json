{
  "grammars": [
    {
      "name": "gaptst",
      "camelcase": "gaptst",
      "scope": "source.gaptst",
      "path": ".",
      "file-types": [
        "tst"
      ],
      "highlights": "queries/highlights.scm",
      "injections": "queries/injections.scm",
      "injection-regex": "gaptst"
    }
  ],
  "metadata": {
    "version": "0.4.1",
    "license": "MIT",
    "description": "gaptst grammar for tree-sitter",
    "authors": [
      {
        "name": "Reinis Cirpons",
        "email": "rc234@st-andrews.ac.uk"
      }
    ],
    "links": {
      "repository": "https://github.com/gap-system/tree-sitter-gaptst"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
